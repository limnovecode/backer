{
  "name": "Backer",
  "description": "A basic ruby on rails 4.2 application for a Heroku Deployment.",
  "website": "https://backer.lim9.com/",
  "repository": "https://github.com/limnovecode/backer",
  "logo": "https://upload.wikimedia.org/wikipedia/commons/c/c3/Ruby_on_Rails_logo.svg",
  "success_url": "/",
  "scripts": {
    "postdeploy": [
      "bundle exec rake db:setup"
    ]
  },
  "env": {
    "CANONICAL_HOST": "please-edit.herokuapp.com",
    "STORAGE_BUCKET": "please-edit"
  },
  "addons": [
    "heroku-postgresql:hobby-dev"
  ]
}
